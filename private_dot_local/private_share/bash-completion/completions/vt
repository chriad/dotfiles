_vt_root_command()
{
    last_command="vt"

    command_aliases=()

    commands=()
    commands+=("analysis")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("an")
        aliashash["an"]="analysis"
    fi
    commands+=("completion")
    commands+=("domain")
    commands+=("download")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("dl")
        aliashash["dl"]="download"
    fi
    commands+=("file")
    commands+=("group")
    commands+=("help")
    commands+=("hunting")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("ht")
        aliashash["ht"]="hunting"
    fi
    commands+=("init")
    commands+=("ip")
    commands+=("meta")
    commands+=("monitor")
    commands+=("monitorpartner")
    commands+=("retrohunt")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("rh")
        aliashash["rh"]="retrohunt"
    fi
    commands+=("scan")
    commands+=("search")
    commands+=("url")
    commands+=("user")
    commands+=("version")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--apikey=")
    two_word_flags+=("--apikey")
    two_word_flags+=("-k")
    flags+=("--proxy=")
    two_word_flags+=("--proxy")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

__start_vt()
{
    local cur prev words cword
    declare -A flaghash 2>/dev/null || :
    declare -A aliashash 2>/dev/null || :
    if declare -F _init_completion >/dev/null 2>&1; then
        _init_completion -s || return
    else
        __vt_init_completion -n "=" || return
    fi

    local c=0
    local flags=()
    local two_word_flags=()
    local local_nonpersistent_flags=()
    local flags_with_completion=()
    local flags_completion=()
    local commands=("vt")
    local must_have_one_flag=()
    local must_have_one_noun=()
    local has_completion_function
    local last_command
    local nouns=()

    __vt_handle_word
}

if [[ $(type -t compopt) = "builtin" ]]; then
    complete -o default -F __start_vt vt
else
    complete -o default -o nospace -F __start_vt vt
fi

# ex: ts=4 sw=4 et filetype=sh
