_vt_hunting_ruleset()
{
    last_command="vt_hunting_ruleset"

    command_aliases=()

    commands=()
    commands+=("add")
    commands+=("delete")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("del")
        aliashash["del"]="delete"
        command_aliases+=("rm")
        aliashash["rm"]="delete"
    fi
    commands+=("disable")
    commands+=("enable")
    commands+=("list")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("rename")
    commands+=("setlimit")
    commands+=("update")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--exclude=")
    two_word_flags+=("--exclude")
    two_word_flags+=("-x")
    local_nonpersistent_flags+=("--exclude")
    local_nonpersistent_flags+=("--exclude=")
    local_nonpersistent_flags+=("-x")
    flags+=("--identifiers-only")
    flags+=("-I")
    local_nonpersistent_flags+=("--identifiers-only")
    local_nonpersistent_flags+=("-I")
    flags+=("--include=")
    two_word_flags+=("--include")
    two_word_flags+=("-i")
    local_nonpersistent_flags+=("--include")
    local_nonpersistent_flags+=("--include=")
    local_nonpersistent_flags+=("-i")
    flags+=("--threads=")
    two_word_flags+=("--threads")
    two_word_flags+=("-t")
    local_nonpersistent_flags+=("--threads")
    local_nonpersistent_flags+=("--threads=")
    local_nonpersistent_flags+=("-t")
    flags+=("--apikey=")
    two_word_flags+=("--apikey")
    two_word_flags+=("-k")
    flags+=("--proxy=")
    two_word_flags+=("--proxy")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

