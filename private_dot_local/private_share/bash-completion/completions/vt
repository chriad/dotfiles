_vt_domain_immediate_parent()
{
    last_command="vt_domain_immediate_parent"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--cursor=")
    two_word_flags+=("--cursor")
    two_word_flags+=("-c")
    local_nonpersistent_flags+=("--cursor")
    local_nonpersistent_flags+=("--cursor=")
    local_nonpersistent_flags+=("-c")
    flags+=("--exclude=")
    two_word_flags+=("--exclude")
    two_word_flags+=("-x")
    local_nonpersistent_flags+=("--exclude")
    local_nonpersistent_flags+=("--exclude=")
    local_nonpersistent_flags+=("-x")
    flags+=("--identifiers-only")
    flags+=("-I")
    local_nonpersistent_flags+=("--identifiers-only")
    local_nonpersistent_flags+=("-I")
    flags+=("--include=")
    two_word_flags+=("--include")
    two_word_flags+=("-i")
    local_nonpersistent_flags+=("--include")
    local_nonpersistent_flags+=("--include=")
    local_nonpersistent_flags+=("-i")
    flags+=("--limit=")
    two_word_flags+=("--limit")
    two_word_flags+=("-n")
    local_nonpersistent_flags+=("--limit")
    local_nonpersistent_flags+=("--limit=")
    local_nonpersistent_flags+=("-n")
    flags+=("--apikey=")
    two_word_flags+=("--apikey")
    two_word_flags+=("-k")
    flags+=("--proxy=")
    two_word_flags+=("--proxy")
