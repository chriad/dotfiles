    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--apikey=")
    two_word_flags+=("--apikey")
    two_word_flags+=("-k")
    flags+=("--proxy=")
    two_word_flags+=("--proxy")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

